<?xml version="1.0" encoding="UTF-8"?>
<fhir xmlns="http://www.w3.org" 
			 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://www.w3.org fhir-mapping.xsd" >					 
	<resources>	 
		<resource source="org.hl7.fhir.dstu3.model.Patient" target="com.frt.dr.model.base.Patient">
            <attribute source="id" source-type="java.lang.String" target="id" target-type="java.lang.String" cardinality="1..1" />
            <attribute source="gender" source-type="java.lang.String" target="gender" target-type="java.lang.String" cardinality="1..1"/>	
		</resource>
	</resources>	 
	<complexes>	 
		<complex source="org.hl7.fhir.dstu3.model.HumanName" target="com.frt.dr.model.base.PatientHumanName">
            <attribute source="family" source-type="java.lang.String" target="family" target-type="java.lang.String" cardinality="1..1"/>
		</complex>
	</complexes>	 
</fhir>        
